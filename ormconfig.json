{
	"type": "postgres",
	"port": 5432,
	"database": "gitgab-db",
	"synchronize": true,
	"logging": true,
	"entities": ["src/entity/**/*.ts", "dist/src/entity/**/*.js"],
	"migrations": ["src/migration/**/*.ts", "dist/src/migration/**/*.js"],
	"subscribers": ["src/subscriber/**/*.ts", "dist/src/subscribers/**/*.js"],
	"cli": {
		"entitiesDir": "src/entity",
		"migrationsDir": "src/migration",
		"subscribersDir": "src/subscriber"
	}
}
